{
  "name": "@jakzo/aoc",
  "description": "CLI for reading, running and submitting Advent of Code challenges.",
  "version": "0.2.0",
  "main": "dist/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "aoc": "dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf \"./dist\" *.tsbuildinfo",
    "dev": "ts-node ./src/bin.ts",
    "test": "yarn build --noEmit",
    "release": "yarn build:clean && yarn build && changeset publish"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.4.3",
    "cli-html": "^1.8.2",
    "date-fns": "^2.16.1",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "keytar": "^7.2.0",
    "rimraf": "^3.0.2",
    "string-argv": "^0.3.1",
    "tempy": "^1.0.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.12.0",
    "@types/cheerio": "^0.22.22",
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.10",
    "@types/yargs": "^15.0.11",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
